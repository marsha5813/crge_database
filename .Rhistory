library(dplyr)
library(glue)
area <- "Korea"
period <- "1900-1910"
topic_filter <- "Agency"
section_filter <- "Christian or Pro-Christian Leaders"
df <- read.csv("../../../../data.csv", stringsAsFactors = FALSE)
df_sub <- df %>% filter(area == area, period == period, topic == topic_filter, section == section_filter)
print_text <- function(txt) {
if (length(txt) == 0 || all(is.na(txt)) || identical(txt, character(0))) {
cat("No entry\n")
} else {
cat(paste(txt, collapse = "\n\n"), "\n")
}
}
#| label: render_korea_1900_1910_christian_leaders
#| results: asis
#| echo: false
txts <- df_sub %>% pull(text_en)
txts_nonempty <- txts[!is.na(txts) & nzchar(txts)]
if (length(txts_nonempty) > 0) {
print_text(txts_nonempty)
} else {
cat("No content available for this section.\n")
}
